<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net8.0;net9.0;netstandard2.1;netstandard2.0;</TargetFrameworks>
	  <Nullable>enable</Nullable>
	  <Description>Fast and efficient implementation of a Priority Queue.</Description>
	  <Copyright>Copyright © ReconArt, Inc. 2008-2024</Copyright>
	  <Authors>ReconArt, Inc.</Authors>
	  <Company>ReconArt, Inc.</Company>
	  <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
	  <RepositoryUrl>https://github.com/ReconArt/priority-queue</RepositoryUrl>
	  <PublishRepositoryUrl>true</PublishRepositoryUrl>
	  <RepositoryType>git</RepositoryType>
	  <PackageTags>ReconArt Priority Queue PriorityQueue</PackageTags>
	  <PackageVersion>1.0.3</PackageVersion>
	  <GenerateDocumentationFile>true</GenerateDocumentationFile>
	  <PackageReadmeFile>PACKAGE.md</PackageReadmeFile>
	  <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

	<ItemGroup>
		<None Include="PACKAGE.md" Pack="true" PackagePath="\" />
	</ItemGroup>
	
  <ItemGroup>
    <PackageReference Include="Nullable" Version="1.3.1" PrivateAssets="all" />
  </ItemGroup>

  <PropertyGroup Condition="'$(TargetFramework)' == 'netstandard2.0'">
    <LangVersion>8.0</LangVersion>
  </PropertyGroup>

</Project>
